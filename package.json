{
	"name": "rsvpizza",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"debug": "NODE_OPTIONS='--inspect' next dev",
		"postinstall": "prisma generate",
		"lint": "next lint",
		"format": "prettier --write .",
		"lint:ts": "tsc --noEmit",
		"check": "npm run lint && npm run lint:ts && npm run format",
		"prepare": "husky install",
		"start": "next start",
		"angry": "npx rimraf node_modules package-lock.json .next && npm install",
		"full-angry": "npm run angry && npm run check && npm run build && npm run dev"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": [
			"prettier --write",
			"eslint --fix"
		],
		"*": "tsc -p tsconfig.json --noEmit --skipLibCheck"
	},
	"dependencies": {
		"@chakra-ui/react": "^2.5.5",
		"@clerk/nextjs": "^4.14.1",
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@prisma/client": "^4.11.0",
		"@tanstack/react-query": "^4.28.0",
		"@trpc/client": "^10.18.0",
		"@trpc/next": "^10.18.0",
		"@trpc/react-query": "^10.18.0",
		"@trpc/server": "^10.18.0",
		"framer-motion": "^10.10.0",
		"next": "^13.2.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"superjson": "1.12.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/eslint": "^8.21.3",
		"@types/node": "^18.15.5",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-config-next": "^13.2.4",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.7",
		"prisma": "^4.11.0",
		"typescript": "^5.0.2"
	},
	"ct3aMetadata": {
		"initVersion": "7.10.2"
	}
}
